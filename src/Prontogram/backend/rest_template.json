{
    "auth_login": {
        "method": "post",
        "template": "/api/auth/{userId}/login"
    },
    "auth_logout": {
        "method": "post",
        "template": "/api/auth/{userId}/logout"
    },
    "user_signup": {
        "method": "post",
        "template": "/api/users/{userId}"
    },
    "getMessages": {
        "method": "get",
        "template": "/api/users/{userId}/messages?{sid}"
    },
    "sendMessage": {
        "method": "post",
        "template": "/api/users/{receiverUserId}/messages"
    }
}
